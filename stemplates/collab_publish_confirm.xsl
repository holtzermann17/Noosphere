<xsl:template match="collab_publish_confirm">

	<xsl:call-template name="paddingtable">
		<xsl:with-param name="content">
		
			<xsl:call-template name="makebox">
				<xsl:with-param name="title">Confirm Collaboration Publication</xsl:with-param>
				<xsl:with-param name="content">

					<table width="100%" cellpadding="4"><tr><td>

					Publishing will make this collaboration world-readable so you can share it with others. Do you want to do this?

					<p />

					<center>
						
						<font size="+1"><a href="{/globals/main_url}/?op=collab_publish&amp;id={id}">Yes!</a></font>
					</center>
					
					</td></tr></table>

				</xsl:with-param>
			</xsl:call-template>
		</xsl:with-param>
	</xsl:call-template>
</xsl:template>
