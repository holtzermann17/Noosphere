 <!-- the html -->
 <title><NS:template sitename/>: <NS:template NoosphereTitle/></title>
 </head>
 <body BGCOLOR="#FFFFFF" vlink="#0000ff" alink="#ff0000" text="#000000">
  
 <!-- header -->
 <NS:template header raw/>
 
  <!-- content and sidebar -->
  <table WIDTH="100%" CELLPADDING="0" CELLSPACING="0">
   <tr>
    <td VALIGN="TOP">
     <NS:template sidebar raw/>
    </td>
    <td VALIGN="TOP" ALIGN="LEFT" WIDTH="100%">
     <NS:template content raw/>
    </td>
   </tr>
  </table>
 </body>
</html>
